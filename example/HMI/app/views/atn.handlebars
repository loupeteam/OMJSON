    <div class="webhmi-text-value" data-var-name="AtnHmi:readState[0].Status[4]"></div>
    <div class="webhmi-text-value" data-var-name="AtnHmi:readState[0].Status[5]"></div>
    <div class="webhmi-text-value" data-var-name="AtnHmi:readState[0].Status[6]"></div>
